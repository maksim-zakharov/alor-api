{
  "name": "alor-api",
  "version": "1.0.49",
  "description": "Node.js SDK for Alor Open API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "preversion": "tsc",
    "build": "tsc",
    "postversion": "npm publish",
    "prepare": "husky install",
    "pre-commit": "lint-staged --diff=origin/master",
    "generate-models": "sta -p https://alor.dev/rawdocs2/WarpOpenAPIv2.yml -o ./src/models -n models.ts --no-client -r --extract-request-body --extract-request-params"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maksim-zakharov/alor-api.git"
  },
  "keywords": [
    "alor",
    "typescript",
    "openapi"
  ],
  "author": "Maksim Zakharov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maksim-zakharov/alor-api/issues"
  },
  "homepage": "https://github.com/maksim-zakharov/alor-api#readme",
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "dotenv": "^16.3.1",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.2",
    "swagger-typescript-api": "^13.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.ts": [
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^1.4.0",
    "jwt-decode": "^4.0.0",
    "ms": "^2.1.3",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  }
}
